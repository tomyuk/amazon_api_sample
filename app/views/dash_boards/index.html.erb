<p id="notice"><%= notice %></p>

<h1>Listing Dash Boards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dash_boards.each do |dash_board| %>
      <tr>
        <td><%= link_to 'Show', dash_board %></td>
        <td><%= link_to 'Edit', edit_dash_board_path(dash_board) %></td>
        <td><%= link_to 'Destroy', dash_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dash board', new_dash_board_path %>
